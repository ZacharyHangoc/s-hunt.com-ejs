
<%- include('partials/header') %>

<body
    class="text-center body-signup"
    data-new-gr-c-s-check-loaded="14.1094.0"
    data-gr-ext-installed=""
  >
    <main class="form-signup w-100 m-auto">
      <form>
        <a
          href="/"
          class="d-flex align-items-center justify-content-center text-decoration-none brand-name mb-5"
        >
          <span class="fs-4">Solopreneur Hunt</span>
        </a>
        <h1 class="h4 mb-3">Register here</h1>

        <div class="container-fluid d-flex">
          <div class="form-floating d-flex">
            <input
              type="text"
              class="form-control topleftinput"
              id="floatingInput"
              placeholder="Alan"
            />
            <label for="floatingInput">First Name</label>
          </div>
          <div class="form-floating d-flex">
            <input
              type="text"
              class="form-control toprightinput"
              id="floatingInput"
              placeholder="Smith"
            />
            <label for="floatingInput">Last Name</label>
          </div>
        </div>

        <div class="form-floating">
          <input
            type="email"
            class="form-control middleinput"
            id="floatingInput"
            placeholder="name@example.com"
          />
          <label for="floatingInput">Email</label>
        </div>
        <div class="form-floating">
          <input
            type="password"
            class="form-control"
            id="floatingPassword"
            placeholder="Password"
          />
          <label for="floatingPassword">Password</label>
        </div>

        <button class="w-100 btn btn-lg btn-warning" type="submit">
          Sign up
        </button>
        <p class="mt-5 mb-3 text-muted">Â© Solopreneurhunt.com</p>
      </form>
    </main>
  


<%- include('partials/footer') %>
